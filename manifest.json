{
    "manifest_version": 2,
    "name": "My Cool Extension",
    "version": "0.1",
    "content_scripts": [
      {
        "matches": [
          "https://www.instagram.com/p/*"
        ],
        "js": ["jquery-3.5.1.min.js", "content.js"]
      }
    ],
    "browser_action": {
      "default_icon": "icon.png"
    },
    "background": {
      "scripts": ["background.js"]
    },
    "permissions": [
      "storage",
      "downloads"
  ]
}